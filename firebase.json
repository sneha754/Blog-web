{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "firebase": {
    "auth": {
      "signInMethods": [
        "emailLink",
        "password"
      ]
    }
  }
}
